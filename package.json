  },
  "bugs": {
    "url": "https://github.com/ylabonte/procon-ip/issues"
  },
  "main": "index.js",
  "readmeFilename": "README.md",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "",
    "format": "npx prettier --write \"src/**/*.ts\"",
